Space complexity = O(k).
class Solution {
    public int smallestRepunitDivByK(int k) {
        if(k%2 == 0 || k%5 == 0){
            return -1;
        }
        //i = length;
        int num =1;
        int rem =0;
        for(int i=1;i<=k;i++){
            rem = num%k;
            if(rem== 0){
                return i;
            }
            else{
                rem = rem*10 +1;
                num = rem;
            }
        }
        return -1;

    }
}