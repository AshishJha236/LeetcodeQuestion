class Solution {
    public int countCollisions(String directions) {
        int len =directions.length();
        int i=0,j=len-1,count=0;
        while(i<len && directions.charAt(i) =='L'){
            i++;
        }
        while(j>=0 && directions.charAt(j) =='R'){
            j--;
        }
        while(i<=j){
            if(directions.charAt(i) !='S'){
                count++;
            }
            i++;
        }
        return count;
    }
}