class Solution {
    public TreeNode sortedArrayToBST(int[] nums) {
        return CreateBST(nums,0,nums.length-1);
    }
    public TreeNode CreateBST(int[] nums, int sr, int ed){
        if(sr>ed){
            return null;
            }
        int mid = ed+(sr-ed)/2;
        TreeNode root = new TreeNode(nums[mid]);
        root.left = CreateBST(nums,sr,mid-1);
        root.right = CreateBST(nums,mid+1,ed);



        return root;
    }
}